{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5200",
    "build": "pnpm run type-check && vite build",
    "lint": "pnpm lint:js && pnpm lint:styles",
    "lint:fix": "pnpm lint:js:fix && pnpm lint:styles:fix",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "lint:styles": "stylelint \"src/**/*.css\"",
    "lint:styles:fix": "stylelint \"src/**/*.css\" --fix",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "pre-commit": "pnpm run lint:js && pnpm run lint:styles && pnpm run type-check && pnpm run format:check",
    "biome:version": "biome --version",
    "biome:lsp": "biome lsp-proxy"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@tanstack/react-db": "^0.1.22",
    "@types/react-syntax-highlighter": "^15.5.13",
    "classnames": "^2.5.1",
    "event-source-polyfill": "^1.0.31",
    "lucide-react": "^0.544.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.9.2",
    "react-syntax-highlighter": "^15.6.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@eslint/js": "^9.36.0",
    "@types/event-source-polyfill": "^1.0.5",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@vitejs/plugin-react": "^5.0.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "postcss-nesting": "^13.0.2",
    "stylelint": "^16.24.0",
    "stylelint-config-standard": "^36.0.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.7"
  },
  "pnpm": {
    "packageExtensions": {
      "@radix-ui/react-dialog@*": {
        "peerDependencies": {
          "react": "^16.8 || ^17 || ^18 || ^19",
          "react-dom": "^16.8 || ^17 || ^18 || ^19"
        }
      },
      "@radix-ui/react-dropdown-menu@*": {
        "peerDependencies": {
          "react": "^16.8 || ^17 || ^18 || ^19",
          "react-dom": "^16.8 || ^17 || ^18 || ^19"
        }
      },
      "@radix-ui/react-label@*": {
        "peerDependencies": {
          "react": "^16.8 || ^17 || ^18 || ^19",
          "react-dom": "^16.8 || ^17 || ^18 || ^19"
        }
      },
      "@radix-ui/react-progress@*": {
        "peerDependencies": {
          "react": "^16.8 || ^17 || ^18 || ^19",
          "react-dom": "^16.8 || ^17 || ^18 || ^19"
        }
      },
      "@radix-ui/react-select@*": {
        "peerDependencies": {
          "react": "^16.8 || ^17 || ^18 || ^19",
          "react-dom": "^16.8 || ^17 || ^18 || ^19"
        }
      },
      "@radix-ui/react-tabs@*": {
        "peerDependencies": {
          "react": "^16.8 || ^17 || ^18 || ^19",
          "react-dom": "^16.8 || ^17 || ^18 || ^19"
        }
      },
      "@radix-ui/react-toast@*": {
        "peerDependencies": {
          "react": "^16.8 || ^17 || ^18 || ^19",
          "react-dom": "^16.8 || ^17 || ^18 || ^19"
        }
      },
      "@radix-ui/react-icons@*": {
        "peerDependencies": {
          "react": "^16.8 || ^17 || ^18 || ^19",
          "react-dom": "^16.8 || ^17 || ^18 || ^19"
        }
      }
    }
  }
}
