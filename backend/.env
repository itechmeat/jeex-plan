# JEEX Plan Backend Environment Configuration
# Development settings

# Application Settings
ENVIRONMENT=development
SECRET_KEY=dev-secret-key-not-for-production
DEBUG=true

# Database Configuration
DATABASE_URL=postgresql+asyncpg://postgres:secure_password@postgres:5432/jeex_plan

# Redis Configuration
REDIS_URL=redis://redis:6379
REDIS_PASSWORD=redis_password

# Qdrant Vector Database
QDRANT_URL=http://qdrant:6333
QDRANT_API_KEY=
QDRANT_COLLECTION=jeex_plan_documents

# HashiCorp Vault
VAULT_ADDR=http://vault:8200
VAULT_TOKEN=__REPLACE_WITH_TOKEN__

# Authentication
ACCESS_TOKEN_EXPIRE_MINUTES=30
REFRESH_TOKEN_EXPIRE_DAYS=7
ALGORITHM=HS256

# Rate Limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=60

# LLM API Keys
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
DEFAULT_LLM_MODEL=gpt-4

# Observability
OTLP_ENDPOINT=http://localhost:4318
LOG_LEVEL=INFO

# File Storage
UPLOAD_DIR=./uploads
EXPORT_DIR=./exports

# Multi-tenancy
DEFAULT_TENANT_ID=default
ENABLE_TENANT_ISOLATION=true

# CORS Settings
ALLOWED_ORIGINS=["http://localhost:5200","http://localhost:3000","http://localhost:8080"]
